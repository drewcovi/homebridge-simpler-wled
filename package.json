{
  "name": "homebridge-wled-ts",
  "version": "1.0.0",
  "description": "Homebridge plugin for WLED LED controllers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:ui",
    "build:ui": "tsc -p homebridge-ui/tsconfig.json && cp -r homebridge-ui/public dist/homebridge-ui/",
    "prepublishOnly": "npm run build",
    "watch": "npm run build -- --watch",
    "lint": "eslint src/**.ts",
    "test": "echo \"No tests specified\""
  },
  "engines": {
    "node": ">=14.0.0",
    "homebridge": ">=1.3.0"
  },
  "keywords": [
    "homebridge-plugin",
    "wled",
    "led",
    "light",
    "esp8266",
    "esp32",
    "homekit"
  ],
  "author": "Drew Covi",
  "license": "MIT",
  "homepage": "https://github.com/drewcovi/homebridge-wled-ts#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/drewcovi/homebridge-wled-ts.git"
  },
  "bugs": {
    "url": "https://github.com/drewcovi/homebridge-wled-ts/issues"
  },
  "homebridge-plugin-ui": {
    "pluginAlias": "WLED",
    "customUiPath": "./dist/homebridge-ui"
  },
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^1.0.3",
    "axios": "^1.6.0",
    "dnssd": "^0.4.1",
    "node-persist": "^3.1.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/dnssd": "^0.4.5",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "homebridge": "^1.6.0",
    "typescript": "^5.0.0"
  }
}
